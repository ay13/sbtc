header
  .container
    .row
      nav.navbar.navbar-default(role='navigation')
        .container-fluid
          .navbar-header
            .navbar-brand
              a(href='/')
                h1 Logo Here
          .menu
            ul.nav.nav-tabs(role='tablist')
              each(loop='item in ["programs"]')
                li(class="{{ pageId === item ? 'active' : 'inactive'}}" role='presentation')
                  a(href='{{ item }}') {{ item }}
